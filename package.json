  "license": "ISC",
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "heroku": "^7.7.8",
    "kill-port": "^2.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.17.12",
    "dotenv": "^16.0.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.4"
  },
  "engines": {
    "node": "v16.15.0"
  }
